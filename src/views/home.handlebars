<g>
	{{#if condition}}

	{{>largeBackground}}

	<text transform="matrix(1 0 0 1 76.9998 27)"
		style="fill:#E8E8E8; font-family:'Arial'; font-size:20;">{{username}}</text>

	{{>smallBackground}}

	<text transform="matrix(1 0 0 1 310.3206 25)"
		style="fill:{{#if darkMode}}#AAAAAA{{else}}#1D1D1F{{/if}}; font-family:'Arial'; font-size:14;">{{honor}}</text>

	{{>logo}}

	{{>kyuLable}}

	{{else}}

	<text transform="matrix(1 0 0 1 310.3206 25)"
		style="fill:{{#if darkMode}}#AAAAAA{{else}}#1D1D1F{{/if}}; font-family:'Arial'; font-size:14;">no data</text>

	{{/if}}
</g>