{
  "name": "codewars-stats",
  "private": "true",
  "version": "1.0.0",
  "description": "codewars user stats as beautifull target for github READMEs",
  "main": "app.js",
  "scripts": {
    "__build__": "rm -rf dist && webpack ./webpack.config.js -p",
    "start": "node ./src/app.js",
    "watch": "npx nodemon ./src/app",
    "test": "npx mocha logic/*.spec.js --timeout: 100000",
    "test-debug": "npx mocha debug logic/*.spec.js",
    "test-inspect": "npx mocha --inspect-brk logic/*.spec.js --timeout: 100000",
    "test:coverage": "nyc --reporter=html --reporter=text mocha logic/**/**/*.spec.js --runInBand"
  },
  "author": "Christian Haag",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-fetch": "^0.3.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "handlebars-webpack-plugin": "^2.1.1",
    "html-webpack-plugin": "^4.5.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2"
  }
}